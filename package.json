{
  "name": "car-smile-mngr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install": ".git-hooks/install-hooks.sh",
    "build": "tsc",
    "start": "npm run build && export DEBUG=bdb:* && node ./build/bin/Server.js",
    "dev": "nodemon --watch src -e ts,js --exec npm start",
    "test:file": "export $(cat test/.env-test | xargs) && mocha --timeout 10000 --require ts-node/register",
    "test:file:example": "npm run test:file -- 'test/example.spec.ts' -g 'Test Class Name' ",
    "test:only": "npm run test:file -- 'test/**/*.spec.ts'",
    "test": "npm run lint && npm run test:only",
    "coverage": "export $(cat test/.env-test | xargs) && nyc _mocha --timeout 10000 --require ts-node/register 'test/**/*.spec.ts'",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "coverage:web": "npm run coverage && http-server coverage/",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "debug": "DEBUG=* node ./build/bin/Server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SmileWebSystem/car-smile-mngr.git"
  },
  "author": "Leonardo Valbuena",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SmileWebSystem/car-smile-mngr/issues"
  },
  "homepage": "https://github.com/SmileWebSystem/car-smile-mngr#readme",
  "dependencies": {
    "@types/easy-soap-request": "^4.1.1",
    "aws-sdk": "~2.766.0",
    "cookie-parser": "~1.4.4",
    "dateformat": "^3.0.3",
    "debug": "~4.1.1",
    "dotenv": "^10.0.0",
    "easy-soap-request": "^4.3.0",
    "express": "~4.16.1",
    "express-actuator": "^1.5.0",
    "express-openapi-validate": "^0.5.0",
    "express-openapi-validator": "^3.9.1",
    "express-validator": "~5.3.1",
    "http-server": "~0.11.1",
    "morgan": "~1.9.1",
    "node-fetch": "~2.6.0",
    "typescript": "~4.3.5",
    "typescript-json-schema": "~0.50.1",
    "ua-parser-js": "~0.7.20"
  },
  "devDependencies": {
    "@types/chai": "~4.1.7",
    "@types/cookie-parser": "~1.4.1",
    "@types/dateformat": "^3.0.1",
    "@types/debug": "~4.1.4",
    "@types/express": "~4.17.0",
    "@types/express-actuator": "^1.3.0",
    "@types/mocha": "~5.2.7",
    "@types/morgan": "~1.7.36",
    "@types/nock": "~10.0.3",
    "@types/node": "^12.12.3",
    "@types/node-fetch": "~2.5.0",
    "@types/sinon": "~7.0.13",
    "@types/ua-parser-js": "~0.7.33",
    "chai": "~4.2.0",
    "chai-http": "~4.3.0",
    "mocha": "~6.1.4",
    "nock": "~10.0.6",
    "nodemon": "~1.19.1",
    "nyc": "~14.1.1",
    "sinon": "~7.4.1",
    "ts-node": "~8.2.0",
    "tslint": "~5.17.0"
  }
}
